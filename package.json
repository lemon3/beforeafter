{
  "name": "beforeafter",
  "version": "1.0.2",
  "description": "before and after image comparison slider",
  "author": "wolfgang jungmayer",
  "license": "MIT",
  "main": "dist/beforeafter.min.js",
  "module": "./dist/beforeafter.esm.min.js",
  "exports": {
    ".": {
      "import": "./dist/beforeafter.esm.min.js",
      "require": "./dist/beforeafter.min.js"
    }
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "lint": "eslint 'src/**/*.js' || true",
    "prettify": "prettier --write 'src/**/*.js' && prettier --write 'src/styles/**/*.css'",
    "dev": "webpack --mode development --watch --config config/webpack.dev.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "browserslistupdate": "npx update-browserslist-db@latest"
  },
  "keywords": [
    "beforeafter",
    "images",
    "slider"
  ],
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/preset-env": "^7.19.1",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.23.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.31",
    "postcss-loader": "^8.1.1",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.6",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
